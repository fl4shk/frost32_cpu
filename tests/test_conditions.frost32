.org 0x0000
main:
{
	// Assume top of available memory is here
	cpyi sp, 0x7fff


	cpya u0, 5
	cpya u1, -5

	; Branch to subroutine
	addsi lr, pc, 8
	bra test_bles


	bra quit
}

test_bles:
{
	bles u0, u1, les

gts:
	cpyi u0, 0
	
	bra done

les:
	cpyi u0, 1


done:
	


	jmp lr
}



.org 0x4000
quit:
{
	bra quit
}
