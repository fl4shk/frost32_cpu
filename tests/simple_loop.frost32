.org 0x0000
main:
{
	;cpyi u0, 0x50		; 0x0
	;cpyi u0, 20			; 0x0
	subi u0, zero, 19		; 0x0
	cpy u1, u0			; 0x4

	subi u2, zero, 20		; 0x8

	cpyi u3, loop


loop:
	inc u1				; 0xc
	
	dec u0				; 0x10
	bges u0, u2, loop	; 0x14
	;jges u0, u2, u3

	;calla test_call		; 0x18
	;addsi lr, pc, 8		; 0x18
	;bra test_call		; 0x1c
	;addsi lr, pc, . - test_call + 8
	;addsi lr, pc, 
	;;cpyi lr, test_call
	addsi lr, pc, test_call - .
	call lr

	bra quit
}

test_call:
{
	cpyi u3, 6
	jmp lr
}

